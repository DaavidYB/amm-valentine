{
  "version": 2,
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "functions": {
    "server/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/server/server.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}